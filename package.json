{
  "name": "@beer-code-industry/common-icons",
  "version": "1.0.0",
  "private": false,
  "description": "Library containing all icons used in the Beer Code Industry ecosystem.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
